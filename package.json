{
  "name": "little-insomniac",
  "version": "1.0.1",
  "description": "A free anti-sleep app that lets you prevent computer sleep on all of your devices!",
  "main": "./dist/index.js",
  "scripts": {
    "test": "npm run prepare && ./node_modules/mocha/bin/mocha test/ --recursive --timeout=10000",
    "prepare": "node -e \"require('prepare-package')()\"",
    "prepare:watch": "nodemon -w ./src -e '*' --exec 'npm run prepare'"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/little-insomniac/little-insomniac"
  },
  "keywords": [
    "little-insomniac",
    "anti-sleep app",
    "prevent computer sleep",
    "nodejs",
    "front end"
  ],
  "author": "ITW Creative Works",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/little-insomniac/little-insomniac/issues"
  },
  "homepage": "https://little-insomniac.com",
  "preparePackage": {
    "input": "./src",
    "output": "./dist",
    "replace": {}
  },
  "dependencies": {
    "itwcw-package-analytics": "^1.0.4"
  },
  "devDependencies": {
    "mocha": "^8.4.0",
    "prepare-package": "^1.1.12"
  }
}